  "authors": ["Dr. Rubisco (@thedocruby)"],
  "contact": {
    "sources": "https://github.com/thedocruby/resounding",
    "issues": "https://github.com/thedocruby/resounding/issues",
    "homepage": "https://thedocruby.dev/resounding",
    "wiki": "https://github.com/thedocruby/resounding/wiki/FAQ",
    "discord": "https://discord.gg/kdZ6gH7Hg6",
    "patreon": "https://patreon.com/thedocruby",
    "modrinth": "https://modrinth.com/mod/resounding",
    "youtube": "https://www.youtube.com/channel/UCONFVOfzRPAISQj2Xbb827w"
  },
  "license": "LGPL-3.0",
  "icon": "assets/resounding/icon.png",
